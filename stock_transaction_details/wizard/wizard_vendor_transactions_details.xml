<odoo>
    <record id="wizard_vendor_transaction_details_form" model="ir.ui.view">
        <field name="name">wizard.vendor.transaction.details.view.form</field>
        <field name="model">wizard.vendor.transaction.details</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="start_date"/>
                        <field name="end_date"/>
                        <field name="warehouse_ids" widget="many2many_tags"/>
                        <field name="partner_ids" widget="many2many_tags"/>
                        <field name="vendor_type"/>
                        <field name="season_ids" widget="many2many_tags"/>
                        <field name="category_ids" widget="many2many_tags"/>

                    </group>
                </sheet>
                <footer>
                    <button name="action_print_report" string="Print" type="object" class="oe_highlight"/>
                    <button string="Cancel" special="cancel" class="oe_link"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="wizard_vendor_transaction_details_action" model="ir.actions.act_window">
        <field name="name">Vendor Transactions Details</field>
        <field name="res_model">wizard.vendor.transaction.details</field>
        <field name="view_mode">form</field>
        <field name="view_type">form</field>
        <field name="target">new</field>
    </record>


    <menuitem id="menu_wizard_vendor_transaction_details"
              name="Vendor Transactions Details"
              parent="purchase.purchase_report"
              action="wizard_vendor_transaction_details_action"/>

</odoo>
